#include<bits/stdc++.h>

using namespace std;
int main(){
	int   t;
  long long a,b,c,d;
	cin>>t;
	while(t--){
	
		cin>>a>>b>>c>>d;
		if(a==b){
			cout<<"YES"<<endl;
		}
		else if(a!=b && c==d){
			cout<<"NO"<<endl;
		}
		else if(abs(a-b)%abs(c-d)==0){
				cout<<"YES"<<endl;
			}
		else{
			cout<<"NO"<<endl;
		}
	}
}
